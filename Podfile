	pod 'GoogleAPIClientForREST/Drive', "2.0.0"
	pod 'GTMAppAuth',"1.3.0"
	pod 'SDWebImageSwiftUI',"~> 2.0"

  target 'CheckinTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'GoogleSignIn',"~> 6.2"
	pod 'GoogleSignInSwiftSupport',"~> 6.2.2"
	pod 'GoogleAPIClientForREST/Sheets', "2.0.0"
	pod 'GoogleAPIClientForREST/Drive', "2.0.0"
	pod 'GTMAppAuth',"1.3.0"
	pod 'SDWebImageSwiftUI',"~> 2.0"
  end

  target 'CheckinUITests' do
    # Pods for testing
  end

end
